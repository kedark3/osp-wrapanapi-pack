{
"name": "osp-wrapanapi",

"description": "Checks for Red Hat OpenStack using wrapanapi",

"path":"cloud/",

"templates": ["OSP"],

"macros":{
         "_OSP_VM_COUNT_WARN": {"type":"integer",
                             "description":"Warning Number of VMs in OSP"
                             },
         "_OSP_VM_COUNT_CRIT": {"type":"integer",
                             "description":"Critical Number of VMs in OSP"
                             },
         "_OSP_KEYPAIR_COUNT_WARN": {"type":"integer",
                             "description":"Warning Number of Keypairs in OSP"
                             },
         "_OSP_KEYPAIR_COUNT_CRIT": {"type":"integer",
                             "description":"Critical Number of Keypairs in OSP"
                             },
         "_OSP_VOLUME_COUNT_WARN": {"type":"integer",
                             "description":"Warning Number of Volumes in OSP"
                             },
         "_OSP_VOLUME_COUNT_CRIT": {"type":"integer",
                             "description":"Critical Number of Volumes in OSP"
                             },
         "_OSP_IMAGE_COUNT_WARN": {"type":"integer",
                             "description":"Warning Number of Images in OSP"
                             },
         "_OSP_IMAGE_COUNT_CRIT": {"type":"integer",
                             "description":"Critical Number of Images in OSP"
                             },
         }
}
